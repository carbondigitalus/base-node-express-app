{
  "name": "slice-budget-app",
  "version": "1.0.0",
  "description": "The Slice business budgeting app lets you know where you money is.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:dev": "NODE_ENV=development nodemon server.js",
    "start:prod": "NODE_ENV=production nodemon server.js",
    "start:test": "NODE_ENV=test nodemon server.js",
    "debug": "ndb server.js",
    "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js --public-url /js",
    "build:js": "parcel build ./public/js/index.js --out-dir ./public/js --out-file bundle.js --public-url /js",
    "test": "cross-env NODE_ENV=development mocha \"./{,!(node_modules)/**/}*.test.js\" --exit",
    "test2": "mocha \"./test/*.js\" --recursive",
    "coverage": "cross-env NODE_ENV=development nyc --reporter=text npm test",
    "style": "sass public/css/scss/style.scss:public/css/style.css",
    "style:min": "sass public/css/scss/style.scss:public/css/style.css --style=compressed",
    "style:watch": "sass --watch public/css/scss/style.scss:public/css/style.css"
  },
  "author": "Carbon Digital",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^1.3.2",
    "express-rate-limit": "^5.0.0",
    "helmet": "^3.21.2",
    "hpp": "^0.2.2",
    "html-to-text": "^5.1.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.27.0",
    "mongoose": "^5.7.7",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.3.1",
    "pug": "^2.0.4",
    "sharp": "^0.23.2",
    "slugify": "^1.3.5",
    "stripe": "^7.13.0",
    "validator": "^12.0.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "ndb": "^1.1.5",
    "nodemon": "^2.0.15",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.18.2"
  },
  "engines": {
    "node": "12.X"
  }
}
